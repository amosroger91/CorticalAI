# OLLAMA-Orchestrator Configuration
SERVER_PORT=3002
SERVER_IP=localhost
SERVER_ENABLE_CORS=true

# App Identity - DuckDuckGo Search
APP_NAME=DuckDuckGo Search Assistant
APP_DESCRIPTION=Search for information, topics, and answers using DuckDuckGo
APP_WELCOMEMESSAGE=Hello! I can help you search for topics and information from DuckDuckGo. What would you like to find?
APP_PRIMARYCOLOR=DE5833
APP_SECONDARYCOLOR=333333

# Function Pattern (optional - uses default if not specified)
APP_FUNCTION_PATTERN=^FUNCTION:(\w+):(.+)$

# OLLAMA Configuration
OLLAMA_ENDPOINT=http://localhost:11434/api/generate
OLLAMA_MODEL=gemma3:1b
OLLAMA_TIMEOUT=900000
OLLAMA_STREAM_TIMEOUT=1200000

# Other env vars (API endpoints)
DUCK_API_ENDPOINT=https://api.duckduckgo.com/
